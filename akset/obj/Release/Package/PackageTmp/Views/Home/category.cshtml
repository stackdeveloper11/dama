                    <th>eskifiyat</th>
                    <th>fiyat</th>
                    <th>link</th>
                    <th>satistami</th>
                    <th>urunkodu</th>
                    <th>urunlefiltres</th>
                    <th>v1</th>
                    <th>v2</th>
                    <th>tarih</th>
                    <th>resim</th>
                </tr>
            </tfoot>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr class="git odd" role="row">
                        <td>
                            <span style="display:none">@item.Id</span>
                            <div style="position:relative;float: left;border: 1px solid #eee;overflow:hidden">
                                <div class="favorite" onclick="addfavor()">
                                    <i class="icon icon-favorite-heart"></i>
                                </div>
                                @{
                                    string linli = "";
                                    if (item.link == "" || item.link == null)
                                    {
                                        linli = akset.helper.Genel.URLFriendly(item.baslik) + "-" + "urunid" + "-" + item.Id;
                                    }
                                    else
                                    {
                                        linli = item.link;
                                    }
                                    string resim = "";
                                    if (item.v2 == "")
                                    {
                                        resim = "<img class='rrtt' src='/urunresimleri/" + item.resim + "/" + item.v1 + "' alt='" + item.baslik + "' style='width:318px;height:497px' />";
                                    }
                                    else
                                    {
                                        resim = "<img class='rrtt' src='/urunresimleri/" + item.resim + "/" + item.v1 + "' alt='" + item.baslik + "' onmouseover='$(this).attr(\"src\",\"/urunresimleri/" + item.resim + "/" + item.v2 + "\")' onmouseout='$(this).attr(\"src\",\"/urunresimleri/" + item.resim + "/" + item.v1 + "\")' style='width:318px;height:497px' />";
                                    }
                                }

                                <a href="@linli">
                                    <div class="rrtt" style="overflow:hidden;width:318px;height:497px">
                                        @Html.Raw(resim)
                                    </div>
                                    <div class="product-info-container" style="width:100%">
                                        <div class="info-box" style="text-align:left">
                                            <div class="brand" style="text-overflow: ellipsis;white-space: nowrap;overflow: hidden;">@item.baslik</div>
                                            <div class="price-container" style="float:right">
                                                @if (item.eskifiyat != "0")
                                                {
                                                    <div class='market-price' style='line-height:30px'>@item.eskifiyat  TL</div>
                                                }
                                                <div class="sale-price" style="font-weight: bolder;">@item.fiyat TL</div>
                                            </div>
                                        </div>
                                    </div>
                                </a>
                            </div>
                        </td>
                        <td class="d-none">@item.adi</td>
                        <td class="d-none">@item.baslik</td>
                        <td class="d-none">@item.eskifiyat</td>
                        <td class="d-none">@item.fiyat</td>
                        <td class="d-none">@item.link</td>
                        <td class="d-none">@item.satistami</td>
                        <td class="d-none">@item.urunkodu</td>
                        <td class="d-none">
                            @foreach (var itemi in item.urunlefiltres)
                            {
                                <div>
                                    @itemi.adi
                                </div>
                                foreach (var itemde in itemi.ozls)
                                {
                                    <div>
                                        @itemde.ozellik
                                    </div>
                                }
                            }
                        </td>
                        <td class="d-none">@item.v1</td>
                        <td class="d-none">@item.v2</td>
                        <td class="d-none">@item.tarih</td>
                        <td class="d-none">@item.resim</td>
                    </tr>
                }
            </tbody>
        </table>
        <div style="opacity:0">
            <div class="dt-more-container">
                <div id="btn-example-load-more1"></div>
            </div>
        </div>
    </div>
</div>
