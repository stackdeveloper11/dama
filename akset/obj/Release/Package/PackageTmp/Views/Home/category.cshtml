@{
    string bir = "";
    string iki = "";
    string uc = "";
    string ktg = "";
    string ack = "";
    int idsim = 0;
    akset.helper.Genel.str = "";

    foreach (var item in aksetDB.Create().Kategoris.ToList())
    {
        if (akset.helper.Genel.URLFriendly(item.baslik) == ViewContext.RouteData.Values["ilk"].ToString())
        {
            bir = item.adi;
            akset.helper.Genel.getparent(item);
            iki = akset.helper.Genel.str;
            ktg = item.icerik;
            ack = item.aciklama;
            idsim = item.Id;
        }
    }

    iki = iki.Substring(0, iki.Length - 1);
    List<KeyValuePair<int, string>> _items = new List<KeyValuePair<int, string>>();
    if (iki.Contains(','))
    {
        int yy = 0;
        string[] fff = iki.Split(',');
        foreach (var item in fff)
        {
            if (item.Contains("["))
            {
                yy++;
                _items.Add(new KeyValuePair<int, string>(yy, item));
            }

        }
    }
}
